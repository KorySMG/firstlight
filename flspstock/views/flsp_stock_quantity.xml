<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record model="ir.ui.view" id="view_stock_quant_tree_onhandqty_inherit">
        <field name="name">stock.quant.tree.onhandqty.inherit</field>
        <field name="model">stock.quant</field>
        <field name="inherit_id" ref="stock.view_stock_quant_tree_editable"></field>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='inventory_quantity']" position="replace">
                <field name="inventory_quantity" string="On Hand Quantity" readonly="1"/>
            </xpath>
        </field>
    </record>
</odoo>
