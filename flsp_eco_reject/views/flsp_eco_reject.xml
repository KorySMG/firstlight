<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!--Form view for the eco-->
    <record id="flsp_eco_reject_form" model="ir.ui.view">
        <field name="name">flsp.eco.reject.form</field>
        <field name="model">mrp.eco</field>
        <field name="inherit_id" ref="mrp_plm.mrp_eco_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='tag_ids']" position="after">
                <field name="backup_pdct" invisible="1"/>
            </xpath>
        </field>
    </record>
</odoo>